{
  "name": "@aethel/aethel-admin-panel",
  "version": "1.0.0",
  "description": "Aethel Admin Panel for user management, payments, and metrics",
  "keywords": [
    "theia-extension",
    "aethel",
    "admin",
    "user-management",
    "payments"
  ],
  "license": "Proprietary Aethel License",
  "repository": {
    "type": "git",
    "url": "https://github.com/aethel/aethel-theia-fork.git"
  },
  "bugs": {
    "url": "https://github.com/aethel/aethel-theia-fork/issues"
  },
  "homepage": "https://aethel-ide.com",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "^1.43.0",
    "@theia/preferences": "^1.43.0",
    "react-admin": "^4.16.0",
    "axios": "^1.6.0",
    "@stripe/stripe-js": "^3.0.0"
  },
  "devDependencies": {
    "@theia/cli": "^1.43.0",
    "typescript": "^5.0.0",
    "rimraf": "^3.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/aethel-admin-panel-frontend-module"
    }
  ],
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "webpack --config ./webpack.config.js",
    "watch": "webpack --config ./webpack.config.js --watch",
    "test": "mocha --config ../../configs/mocha-config.json",
    "lint": "eslint --config ../../configs/.eslintrc.json --ext .ts,.tsx src"
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.15.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "aethel.admin.open",
        "title": "Open Admin Panel",
        "category": "Aethel Admin"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "aethel.admin.open"
        }
      ]
    }
  }
}
