{
  "name": "aethel-desktop-app",
  "version": "0.1.0",
  "private": true,
  "main": "src/main.cjs",
  "scripts": {
    "dev": "electron .",
    "build": "electron-builder --win --x64"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.aethel.desktop",
    "productName": "Aethel IDE",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    }
  },
  "dependencies": {
    "aethel-engine": "file:../.."
  }
}
