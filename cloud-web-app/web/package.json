{
  "name": "aethel-portal",
  "private": true,
  "version": "0.2.0",
  "scripts": {
    "dev": "next dev -p 3000 -H 0.0.0.0",
    "dev:full": "concurrently \"npm run dev\" \"npm run ws\" \"npm run runtime\"",
    "ws": "ts-node --transpile-only server/websocket-server.ts",
    "ws:dev": "nodemon --exec ts-node --transpile-only server/websocket-server.ts",
    "runtime": "cross-env RUNTIME_PORT=3001 ts-node --transpile-only lib/server/bootstrap.ts",
    "runtime:dev": "cross-env RUNTIME_PORT=3001 nodemon --exec ts-node --transpile-only lib/server/bootstrap.ts",
    "worker:build-queue": "ts-node --transpile-only server/workers/build-queue-worker.ts",
    "build": "next build",
    "start": "next start -p 3000 -H 0.0.0.0",
    "start:full": "concurrently \"npm run start\" \"npm run ws\" \"npm run runtime\"",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:fast": "jest --onlyChanged --passWithNoTests",
    "test:changed": "jest --changedSince=HEAD --passWithNoTests",
    "qa:canonical-components": "node scripts/check-canonical-components.mjs",
    "qa:route-contracts": "node scripts/check-route-contracts.mjs",
    "qa:no-fake-success": "node scripts/check-no-fake-success.mjs",
    "qa:mojibake": "node scripts/scan-mojibake.mjs",
    "qa:interface-critical": "node scripts/interface-critical-scan.mjs",
    "qa:interface-gate": "node scripts/interface-critical-gate.mjs",
    "qa:enterprise-gate": "npm run qa:interface-gate && npm run qa:canonical-components && npm run qa:route-contracts && npm run qa:no-fake-success && npm run qa:mojibake && npm run typecheck && npm run build",
    "docs:architecture-triage": "node scripts/architecture-critical-scan.mjs",
    "docs:routes-inventory": "node scripts/generate-routes-inventory.mjs",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "postinstall": "prisma generate && node scripts/fix-prisma-client.cjs"
  },
