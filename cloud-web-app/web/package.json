{
  "name": "aethel-portal",
  "private": true,
  "version": "0.2.0",
  "scripts": {
    "dev": "next dev -p 3001 -H 0.0.0.0",
    "dev:full": "concurrently \"npm run dev\" \"npm run runtime\"",
    "runtime": "ts-node --transpile-only lib/server/bootstrap.ts",
    "runtime:dev": "nodemon --exec ts-node --transpile-only lib/server/bootstrap.ts",
    "build": "next build",
    "start": "next start -p 3001 -H 0.0.0.0",
    "start:full": "concurrently \"npm run start\" \"npm run runtime\"",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:fast": "jest --onlyChanged --passWithNoTests",
    "test:changed": "jest --changedSince=HEAD --passWithNoTests",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "postinstall": "prisma generate && node scripts/fix-prisma-client.cjs"
  },
